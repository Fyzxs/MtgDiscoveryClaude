{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CerberusCosmosConfig": {
    "MtgDiscovery": {
      "auth_mode": "sas",
      "account_name": "your-cosmos-account-name",
      "entra": {
        "tenant_id": "your-tenant-id",
        "client_id": "your-client-id",
        "client_secret": "your-client-secret",
        "scope": "https://cosmos.azure.com/.default"
      },
      "sas": {
        "connection_string": "AccountEndpoint=https://your-cosmos-account.documents.azure.com:443/;AccountKey=your-account-key==;"
      },
      "MtgDiscoveryV4": {
        "throughput": 400,
        "autoscale_max": 4000,
        "time_to_live": -1,
        "SetItems": {
          "partition_key_path": "/code",
          "time_to_live": -1
        },
        "SetAssociations": {
          "partition_key_path": "/parent_set_code",
          "time_to_live": -1
        }
      }
    }
  }
}